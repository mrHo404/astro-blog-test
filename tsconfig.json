{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "importsNotUsedAsValues": "error",
    "baseUrl": ".",
    "paths": {
      "@types": ["src/types/index"],
      "@utils": ["src/utils/index"]
    }
  }
}
